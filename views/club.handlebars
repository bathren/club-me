<!doctype html>

<html>
<head>
  <title>clubMe</title> 
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="//code.jquery.com/jquery.min.js"></script>
  <link href="/css/bootstrap.min.css" rel="stylesheet">
  <link href="/css/bootstrap-theme.min.css" rel="stylesheet">
  <link href="/css/introHCI.css" rel="stylesheet">
  <link href='https://code.cdn.mozilla.net/fonts/fira.css' rel='stylesheet' type='text/css' />
  
  <!-- this is a comment in HTML -->

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
      <![endif]-->         
  </head>

  <body>

    <div class="container">
      <div class="myInfo">
        <div>
          <img class="img-responsive" id ="image" src="{{jsonKey.image}}"/></div>
          <p id="name">{{jsonKey.name}}</p>
          <!--<p id="info">{{jsonKey.info}}</p>-->
          <div><p id="descrip">{{jsonKey.desc}}</p></div>
          <div>
            {{#if jsonKey.fb.length}}
            <a href="{{jsonKey.fb}}">
            {{else}}
            <a href="https://www.facebook.com/">
            {{/if}}
            <img class ="icon" src="{{baseUrl}}/images/facebook.png"/></a>
            {{#if jsonKey.website.length}}
            <a href="{{jsonKey.website}}">
            {{else}}
            <a href="https://www.youtube.com/">
            {{/if}}
            <img class ="icon"src="{{baseUrl}}/images/web.png"/>
            </a>
        </div>
      </div>
     
    <div class="bookmark">
      <a href="#" id="button">
        <span id="bookmark">Bookmark Club</span>
      </a>
    </div>

    <a href="/message" id="message1">
      <div class="message">
        <span id="bookmark">Message Club</span>
      </div>
    </a>
      
    
</div>
</body>

<script src="/js/searchbar.js"></script>
<script>
document.getElementById('button').onclick = (function() {
    event.preventDefault();  
    document.getElementByIDName('bookmark')[0].innerHTML = 'Success!';
});

document.getElementById('message').onclick = (function() {
    event.preventDefault();  
});
</script>
</html>
