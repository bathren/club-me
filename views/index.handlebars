<!doctype html>

<html>
<head>
  <title>clubMe</title> 
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="//code.jquery.com/jquery.min.js"></script>
  <link href="/css/bootstrap.min.css" rel="stylesheet">
  <link href="/css/bootstrap-theme.min.css" rel="stylesheet">
  <link href="/css/introHCI.css" rel="stylesheet">
  <link href='https://code.cdn.mozilla.net/fonts/fira.css' rel='stylesheet' type='text/css' />
  <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=yes">
  <link rel='stylesheet prefetch' href='http://fonts.googleapis.com/css?family=Open+Sans'>
  <link rel="stylesheet" href="css/style.css">

</head>

  <body>

  <div class="cont">
  <div class="demo">
    <div class="login">
      <div class="login__check"><h1>clubMe</h1></div>
      <div class="login__form">
        <div class="login__row">
          <svg class="login__icon name svg-icon" viewBox="0 0 20 20">
            <path d="M0,20 a10,8 0 0,1 20,0z M10,0 a4,4 0 0,1 0,8 a4,4 0 0,1 0,-8" />
          </svg>
          <input type="text" class="login__input name" placeholder="Username"/>
        </div>
        <div class="login__row">
          <svg class="login__icon pass svg-icon" viewBox="0 0 20 20">
            <path d="M0,20 20,20 20,8 0,8z M10,13 10,16z M4,8 a6,8 0 0,1 12,0" />
          </svg>
          <input type="password" class="login__input pass" placeholder="Password"/>
        </div>
        <button type="button" class="login__submit" href="indprofile">Sign in</button>
        <p class="login__signup">Don't have an account? &nbsp;<a href="isignup">Sign up</a></p>
      </div>
    </div>
    <div class="app">
      <div class="app__top">
        <div class="app__menu-btn">
          <span></span>
        </div>
        <svg class="app__icon search svg-icon" viewBox="0 0 20 20">
          <!-- yeap, its purely hardcoded numbers straight from the head :D (same for svg above) -->
          <path d="M20,20 15.36,15.36 a9,9 0 0,1 -12.72,-12.72 a 9,9 0 0,1 12.72,12.72" />
        </svg>
         <div class="app__user">
          <img src="http://ghostlypixels.com/wp-content/themes/illustrate-ghostly/images/profile-pic.png"alt="" class="app__user-photo" />
          <span class="app__user-notif">1</span>
        </div>
        <p class="app__hello">John Doe</p>
        <p id="info">First Year || Revelle || Biology</p>
        <div class="row">
        <div class="col-xs-3"></div>
        <div class="project" id="{{id}}">
          <a href="matchme">
            <img class="col-sm-3" class="icon" src="images/match.png" ... />
            <p id="categories" class="text-center">Match Me</p>
          </a>
          <a href="myclubs">
            <img class="col-sm-3" class="icon" src="images/mine.png" ... />
            <p id="categories" class="text-center">My Clubs</p>
          </a>
          <a href="search" class="app__user-photo">
            <img class="col-sm-3" class="icon" src="images/search.png" ... />
            <p id="categories" class="text-center">Club Search</p>
          </a>
          <a href="update" class="app__user-photo">
            <img class="col-sm-3" class="icon" src="images/prof.png" ... />
            <p id="categories" class="text-center">Update Profile</p>
          </a>
        </div>
      </div>
      </div>
      </div>
      <div class="app__logout">
        <svg class="app__logout-icon svg-icon" viewBox="0 0 20 20">
          <path d="M6,3 a8,8 0 1,0 8,0 M10,0 10,12"/>
        </svg>
      </div>
    </div>
  </div>
</div>
    <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>

        <script src="js/index.js"></script>

  </body>
</html>
